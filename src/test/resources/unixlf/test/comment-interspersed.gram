/* american english */
language en-US;
/* voice mode */
mode voice;
/* the root rule is main */
root $main;
/* begin meta declarations */
meta 'description' is 'ABNF header with comments interspersed.';
meta 'in.1' is 'test';
meta 'out.1' is '$main["test"]';

public $main = test;
