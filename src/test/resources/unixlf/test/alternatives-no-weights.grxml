<!DOCTYPE grammar PUBLIC "-//W3C//DTD GRAMMAR 1.0//EN" 
                  "http://www.w3.org/TR/speech-grammar/grammar.dtd">

<grammar xml:lang="en-US" version="1.0" mode="voice" root="main"
         xmlns="http://www.w3.org/2001/06/grammar"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://www.w3.org/2001/06/grammar 
                             http://www.w3.org/TR/speech-grammar/grammar.xsd"
>

<!-- meta information -->
<meta name="description" content="Implementation Report test -- a set of alternatives with no weights"/>
<meta name="in.1" content="shoulder pads"/>
<meta name="out.1" content='$main["shoulder","pads"]'/>


<!-- rule declarations -->
<!--   hockey equipment for a store. -->
<rule id="main" scope="public">
<example>stick</example>
<example>puck</example>
<example>shoulder pads</example>
<one-of>
  <item>stick</item>
  <item>puck</item>
  <item>jersey</item>
  <item>gloves</item>
  <item>shoulder pads</item>
  <item>elbow pads</item>
  <item>shin guards</item>
</one-of>
</rule>
</grammar>
