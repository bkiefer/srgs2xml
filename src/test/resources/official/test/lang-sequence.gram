language en-US;
mode voice;
root $main;
meta 'description' is 'A language declaration can be applied to a sequence.';
meta 'spec.reference' is 'http://www.w3.org/TR/speech-grammar/#S2.7';
meta 'in.1' is 'my name is André Dubois';
meta 'out.1' is '$main["my","name","is","André","Dubois"]';
public $main = 
 (my name is)  (André Dubois) !fr-CA;

